<div class="resume-well">
  <!-- Now section -->
  <div class="row resume-heading align-center">
    <div class="col-lg-12">What I&#39;m currently doing</div>
  </div>
  <div class="row resume-row" data-bind="with: nowViewModel">
    <div class="col-lg-3 col-xs-3">
      <img data-bind="attr: { src: '/static/img/' + imageName (), title: imageTitle, alt: imageAlt }" class="img-thumbnail"/>
    </div>
    <div class="col-lg-9 col-xs-9">
      <p data-bind="html: description"></p>
    </div>
  </div>
</div>

<hr class="separator"/>
